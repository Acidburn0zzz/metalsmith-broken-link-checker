{
  "name": "metalsmith-broken-link-checker",
  "version": "0.0.0",
  "description": "Metalsmith plugin to check for internal broken links",
  "author": "David Moody <david@davidxmoody.com> (https://davidxmoody.com/)",
  "homepage": "https://github.com/davidxmoody/metalsmith-broken-link-checker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davidxmoody/metalsmith-broken-link-checker.git"
  },
  "main": "lib",
  "scripts": {
    "watch": "coffee --watch -o lib -c src/*.coffee & mocha --watch --compilers coffee:coffee-script/register",
    "test": "mocha --compilers coffee:coffee-script/register",
    "prepublish": "coffee -o lib -c src/*.coffee"
  },
  "keywords": [
    "metalsmith",
    "plugin",
    "metalsmith.io",
    "broken-links",
    "broken-link-checker",
    "link-checker"
  ],
  "devDependencies": {
    "mocha": "~2.2.4",
    "chai": "~2.3.0",
    "coffee-script": "~1.9.2",
    "metalsmith": "~1.7.0"
  },
  "dependencies": {
    "cheerio": "~0.19.0",
    "URIjs": "~1.15.0"
  }
}
